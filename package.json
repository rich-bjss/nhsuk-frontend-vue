{
  "name": "nhsuk-frontend-vue",
  "version": "0.1.4",
  "description": "Unofficail port of NHS UK frontend library to vue.js",
  "private": false,
  "main": "dist/nhsuk-frontend-vue.umd.js",
  "module": "dist/nhsuk-frontend-vue.esm.js",
  "unpkg": "dist/nhsuk-frontend-vue.min.js",
  "repository": "https://github.com/xLasercut/nhsuk-frontend-vue",
  "license": "MIT",
  "files": [
    "dist/*",
    "src/*",
    "!src/preview/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve ./ghpages/src/main.js",
    "deploy": "vue-cli-service build ./ghpages/src/main.js",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-e2e-cypress": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "nhsuk-frontend": "^2.0.0",
    "rollup": "^1.8.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "vue": "^2.6.10",
    "vue-highlightjs": "^1.3.3",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  }
}
