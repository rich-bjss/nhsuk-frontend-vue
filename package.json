{
  "name": "nhsuk-frontend-vue",
  "version": "1.0.2",
  "private": false,
  "description": "Unofficial port of NHS UK frontend library to vue.js",
  "license": "MIT",
  "main": "dist/nhsuk-frontend-vue.umd.js",
  "module": "dist/nhsuk-frontend-vue.esm.js",
  "repository": "https://github.com/xLasercut/nhsuk-frontend-vue",
  "unpkg": "dist/nhsuk-frontend-vue.min.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "lint": "vue-cli-service lint src/**/*",
    "backstop:test": "backstop --config=tests/backstop/config.js test --docker",
    "backstop:clean": "rm -rf tests/backstop/bitmaps_test/*",
    "backstop:ref": "backstop --config=tests/backstop/config.js reference --docker",
    "build:es": "rollup --config config/rollup/rollup.config.js --format es --file dist/nhsuk-frontend-vue.esm.js",
    "build:umd": "rollup --config config/rollup/rollup.config.js --format umd --file dist/nhsuk-frontend-vue.umd.js",
    "build:unpkg": "rollup --config config/rollup/rollup.config.js --format iife --file dist/nhsuk-frontend-vue.min.js",
    "deploy:gh": "cross-env NODE_ENV=ghpages vue-cli-service build",
    "deploy:local": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.1",
    "backstopjs": "^4.1.9",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "nhsuk-frontend": "^2.2.0",
    "rollup": "^1.15.6",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-vue": "^5.0.0",
    "vue": "^2.6.10",
    "vue-highlight.js": "^3.1.0",
    "vue-router": "^3.0.3",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.0.1"
  }
}
