{
  "name": "nhsuk-frontend-vue",
  "version": "0.3.7",
  "description": "Unofficial port of NHS UK frontend library to vue.js",
  "private": false,
  "main": "dist/nhsuk-frontend-vue.umd.js",
  "module": "dist/nhsuk-frontend-vue.esm.js",
  "unpkg": "dist/nhsuk-frontend-vue.min.js",
  "repository": "https://github.com/xLasercut/nhsuk-frontend-vue",
  "license": "MIT",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "deploy:local": "vue-cli-service build",
    "deploy:gh": "cross-env NODE_ENV=ghpages vue-cli-service build",
    "lint": "vue-cli-service lint src/**/*",
    "test:unit": "vue-cli-service test:unit",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/nhsuk-frontend-vue.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/nhsuk-frontend-vue.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/nhsuk-frontend-vue.min.js",
    "backstop:test": "backstop --config=tests/backstop/backstop.js test --docker"
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/vue": "^5.1.9",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "backstopjs": "^4.1.9",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-vue": "^5.0.0",
    "nhsuk-frontend": "^2.1.0",
    "rollup": "^1.14.3",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-vue": "^5.0.0",
    "vue": "^2.6.10",
    "vue-highlightjs": "^1.3.3",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.5.21",
    "vuex": "^3.1.1"
  }
}
